<table class="table table-bordered genre_search_table">
	<thead>
		<tr class="table-head">
			<th class="genre_search_form">ジャンル検索</th>
		</tr>
	</thead>
	<tbody>
			<% genres.each do |genre| %>
			<tr>
        <td class="table-body">
						<%= link_to genre.name, public_items_path(genre_id: genre.id),class:"genre_search_name" %>
				 </td>
			</tr>
			<% end %>
	</tbody>
</table>